        <section class="content-main">
            <div class="row">
                <div class="col-6">
                    <div class="content-header">
                        <h2 class="content-title">Add New Product</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <div class="card mb-4">
                        <div class="card-body">
                         <form action="/admin/banner/add-banner" method="post" enctype="multipart/form-data">
                                    <div class="row">
                                        <div class="col-md-3">
                                            <h6>1.Banner details</h6>
                                        </div>
                                        <div class="col-md-9">                              
                                                <div class="mb-4">
                                                    <label class="form-label">Banner title</label>
                                                    <input type="text" name="bannerTitle" placeholder="Type here" class="form-control">
                                                </div>
                                                <div class="mb-4">
                                                    <label class="form-label">Banner title</label>
                                                    <select name="productRoute" id="" class="form-control">
                                                         <% if(products.length > 0){ %>
                                                                <% products.forEach((product) => { %>
                                                                   <option  value="<%=product._id%>" class="mb-4">                                                 
                                                                        <%=product.productname%>
                                                                    </option>
                                                                <% }) %>
                                                         <% } %>
                                                    </select>
                                                    <!-- <input type="text" name="bannerTitle" placeholder="Type here" class="form-control"> -->
                                                </div>
                                                <div class=" row gx-2">
                                                    <div class="col-md-4 mb-3">
                                                        <label class="form-label">Start date</label>
                                                        <input type="date" name="startDate" class="form-control" placeholder="">
                                                    </div>      
                                                    <div class="col-md-4 mb-3">
                                                        <label class="form-label">End date</label>
                                                        <input type="date"  name="endDate" class="form-control" placeholder="">
                                                    </div>                                     
                                                </div>
                                        </div> 
                                    </div> 
                                    <hr class="mb-4 mt-0">
                                    <div class="row">
                                        <div class="col-md-3">
                                            <h6>2. Add image</h6>
                                        </div>
                                        <div class="col-md-9">
                                            <div class="mb-4">
                                                <label class="form-label">Images</label>
                                                <input class="form-control" type="file" name="image">
                                            </div>
                                            <button type="submit" style="margin-left: 200px;" class="btn btn-md rounded font-sm hover-up"> Add banner </button>
                                        </div> 
                                    </div> 
                         </form>
                    </div>
                    </div>
                </div>
            </div>
            </div>
