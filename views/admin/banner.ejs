                    </div> -->
                </div>
                <div class="card mb-4">
                    <% if(typeof error !== 'undefined'){ %>
                        <h4 class="text-center" style="color: red;"> <%= error %> </h4>
                    <% } %>
                   
                    <header class="card-header">
                        

                        <div class="row align-items-center">
                            <div class="col-lg-2 col-sm-4 col-7 flex-grow-1 col-name">
                                <a class="itemside" href="#"> 
                                   
                                    <div class="info">
                                        <h6 class="mb-0" id="name"> image </h6>
                                    </div>
                                </a>
                            </div>
                            
                            <div class="col-lg-2 col-sm-2 col-2 col-date">
                                <span id="price"> title   </span>
                            </div>
                            <div class="col-lg-2 col-sm-2 col-2 col-date">
                                <span id="price"> start date   </span>
                            </div>
                            <div class="col-lg-2 col-sm-2 col-4 col-price"><span id="category">   end date  </span></div>
                            <div class="col-lg-2 col-sm-2 col-4 col-status">
                                <span class="badge rounded-pill alert-success" id="section"> expired </span>
                            </div>
                            <div class="col-lg-2 col-sm-2 col-3 col-action text-center">
                                <a href="/admin/edit-product" class="btn btn-sm font-sm rounded btn-brand"   > <i class="material-icons md-edit"></i> Actions </a>
                            </div>
                        </div>

                    </header>
                    <!-- card-header end// -->





                    <div class="card-body">
                        <%if(banners.length >0 ){ %>
                            <%banners.forEach((banner) => { %>
                                <div class="row align-items-center">
                                    <div class="col-lg-2 col-sm-4 col-7 flex-grow-1 col-name">
                                        <div class="left">
                                            <img src="/images/banner/<%=banner.image%>" class="img-sm img-thumbnail" alt="Item" />
                                        </div>
                                    </div>
                                    
                                    <div class="col-lg-2 col-sm-2 col-2 col-date">
                                        <span id="price"> 
                                            <%=banner.bannerTitle%> 
                                         </span>
                                    </div>
                                    <div class="col-lg-2 col-sm-2 col-2 col-date">
                                        <span id="price"> 
                                            <%= banner.startDate.toLocaleDateString('en-GB', { day: '2-digit', month: '2-digit', year: '2-digit' }).replace(/\//g, '-') %>  
                                         </span>
                                    </div>
                                    <div class="col-lg-2 col-sm-2 col-4 col-price"><span id="category">
                                        <%= banner.endDate.toLocaleDateString('en-GB', { day: '2-digit', month: '2-digit', year: '2-digit' }).replace(/\//g, '-') %> 

                                        </span></div>
                                    <div class="col-lg-2 col-sm-2 col-4 col-status">
                                        <span class="badge rounded-pill alert-success" id="section">
                                             <% if(banner.isExpired ){ %>
                                                true
                                             <%} else{ %>
                                                false
                                             <% } %>
                                            </span>
                                    </div>
                                    <div class="col-lg-2 col-sm-2 col-3 col-action text-center">
                                        <a href="/admin/edit-product" class="btn btn-sm font-sm rounded btn-brand"   > <i class="material-icons md-edit"></i> unlist </a>
                                    </div>
                                </div>
                                <hr>
                                <% })%>
                        <%}%>
                        <!-- itemlist  .// -->
                    </div>
                    <!-- card-body end// -->
                </div>
              
            </section>
            


           

