                        <!-- itemlist  .// -->
                    <section class="content-main">
                            <div class="content-header">
                                <div>
                                    <h2 class="content-title card-title">Reffer and earn</h2>
                                </div>
                                <div>
                                    <a href="/admin/get-add-reffer">
                                        <input type="button" value="Add coupon" style="color: white; font-weight: 700;" class="form-control bg-success"/>
                                    </a>
                                </div>
                            </div>
                            <div class="card">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="table-responsive">
                                                <table class="table table-hover">
                                                    <thead>
                                                        <tr>
                                                          
                                                            <th>Refferal amount</th>
                                                            <th>Reffered amount</th>
                                                            <th>description</th>
                                                            <th class="text-center">exp-date</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                    <% if(reffer.length > 0){ %>
                                                        <%reffer.forEach((ref) => { %>
                                                                <tr>                                                     
                                                                    <td><b> <%=ref.refferalamount%> </b></td>
                                                                    <td> <%=ref.refferedamount%></td>
                                                                    <td> <%=ref.description%> </td>
                                                                    <td class="text-center">
                                                                      <%= ref.expires.toLocaleDateString('en-GB', { day: '2-digit', month: '2-digit', year: '2-digit' }).replace(/\//g, '-') %> 
                                                                    </td>
                                                                </tr>
                                                                <% })%> 
                                                            <%}else{%>
                                                            <h2>
                                                                No refferals
                                                            </h2>
                                                        <%}%>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                        <!-- .col// -->
                                    </div>
                                    <!-- .row // -->
                                </div>
                                <!-- card body .// -->
                            </div>
                            <!-- card .// -->
                        </section>
