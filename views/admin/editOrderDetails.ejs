                                                <td>
                                                    <a class="itemside" href="#">
                                                        <div class="left">
                                                            <img src="/images/product/<%=order[0].productDetails.images[0]%>" width="40" height="40" class="img-xs" alt="Item" />
                                                        </div>
                                                        <div class="info">Sweet & Salty Kettle Corn</div>
                                                    </a>
                                                </td>
                                                <td>900</td>
                                                <td><%=order[0].orderItems.quantity%></td>
                                                <td><%=order[0].price%></td>
                                                <td><%=order[0].productDetails.offer%> % </td>
                                            </tr>
                                           
                                        </tbody>
                                    </table>
                                </div> 
                                <!-- table-responsive// -->
                            </div>
                            <!-- col// -->
                            <div class="col-lg-1"></div>
                            <div class="col-lg-4">
