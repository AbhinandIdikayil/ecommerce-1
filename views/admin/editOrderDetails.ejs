                <div class="card">
                   
                    <!-- card-header end// -->
                    <div class="card-body">
                        <div class="row mb-10 mt-10 order-info-wrap">
                            <div class="col-md-4">
                                <article class="icontext align-items-start">
                                    <span class="icon icon-sm rounded-circle bg-primary-light">
                                        <i class="text-primary material-icons md-person"></i>
                                    </span>
                                    <div class="text">
                                        <h6 class="mb-1">Customer</h6>
                                        <p class="mb-1">
                                            <%=order[0].user_Details.firstname%> <br />
                                            <%=order[0].user_Details.email%> <br />
                                            
                                        </p>
                                        <a href="#">View profile</a>
                                    </div>
                                </article>
                            </div>
                            <!-- col// -->
                            <div class="col-md-4">
                                <article class="icontext align-items-start">
                                    <span class="icon icon-sm rounded-circle bg-primary-light">
                                        <i class="text-primary material-icons md-local_shipping"></i>
                                    </span>
                                    <div class="text">
                                        <h6 class="mb-1">Order info</h6>
                                        <p class="mb-1">
                                            Shipping: Fargo express <br />
                                            Pay method: <%=order[0].paymentMethod%> <br />
                                            Status: 
                                        </p>
                                        <a href="#">Download info</a>
                                    </div>
                                </article>
                            </div>
                            <!-- col// -->
                            <div class="col-md-4">
                                <article class="icontext align-items-start">
                                    <span class="icon icon-sm rounded-circle bg-primary-light">
                                        <i class="text-primary material-icons md-place"></i>
                                    </span>
                                    <div class="text">
                                        <h6 class="mb-1">Deliver to</h6>
                                        <p class="mb-1">
                                             <%=order[0].address.housename%> <br /> <%=order[0].address.address%> <br />
                                            CIty:<%=order[0].address.districtORcity%> -
                                            <%=order[0].address.state%>
                                        </p>
                                        <a href="#">View profile</a>
                                    </div>
                                </article>
                            </div>
                            <!-- col// -->
                        </div>
                        <header class="card-header">
                            <div class="row  align-items-center">
                                <div class="col-lg-6 col-md-6 mb-lg-0 mb">
                                    <span> <i class="material-icons md-calendar_today"></i> <b> 
                                        <%= order[0].orderDate.toLocaleDateString('en-GB', { day: '2-digit', month: '2-digit', year: '2-digit' }).replace(/\//g, '-') %>  

                                     </b> </span> <br />
                                    <small class="text-muted">
                                        <% if(order[0].orderDate.getHours() >= 12){ %>
                                            <%=order[0].orderDate.getHours()%>:<%=order[0].orderDate.getMinutes()%>:PM
                                          <% }else{ %>
                                            <%=order[0].orderDate.getHours()%>:<%=order[0].orderDate.getMinutes()%>:AM
                                          <% } %>
                                    </small>
                                </div>
                                <div class="col-lg-6 col-md-6 ms-auto text-md-end">
                                    <% if(order[0].orderItems.orderStatus !== 'cancelled'){ %>
                                        <form action="/admin/order/edit-order-status/<%=order[0]._id%>?proId=<%=order[0].orderItems.productId%>" method="post">
                                            <select name="orderStatus" class="form-select d-inline-block mb-lg-0 mr-5 mw-200">
                                                <option value=""><%=order[0].orderItems.orderStatus%></option>
                                                <%   if(order[0].orderItems.orderStatus === 'shipped'){ %>
                                                    <option>pending</option>
                                                    <option>ordered</option>
                                                    <option>delivered</option>
                                                <%  }else if(order[0].orderItems.orderStatus === 'pending'){ %>
                                                    <option>ordered</option>
                                                    <option>shipped</option>
                                                    <option>delivered</option>
                                                <%  }else if(order[0].orderItems.orderStatus === 'ordered'){ %>
                                                    <option>pending</option>
                                                    <option>shipped</option>
                                                    <option>delivered</option>
                                                <%   }else if(order[0].orderItems.orderStatus === 'delivered'){ %>
                                                    <option>pending</option>
                                                    <option>ordered</option>
                                                    <option>shipped</option>
                                                <%   }else{ %>
                                                    <option value="">cancelled</option>
                                                <%  } %>            
                                            </select>
                                            <button type="submit" class="btn btn-primary">
                                                Save
                                            </button>
                                        </form>
                                    <% }else{ %>
                                        <select name="orderStatus" class="form-select d-inline-block mb-lg-0 mr-5 mw-200">
                                            <option>cancelled</option>   
                                        </select>
                                    <% } %>
                                    
                                   
                                    
                                   
                                </div>
                            </div>
                        </header>
                        <!-- row // -->
                        <div class="row">
                            <div class="col-lg-10">
                                <div class="table-responsive">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th width="40%">Product</th>
                                                <th width="20%">Unit Price</th>
                                                <th width="20%">Quantity</th>
                                                <th width="20%" >Total</th>
                                                <th width="20%">offer%</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            
